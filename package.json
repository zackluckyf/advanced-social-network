  "scripts": {
    "ng": "ng",
    "apidoc": "apidoc -i server/ -o apidoc/ && http-server apidoc/",
    "update-documentation": "apidoc -i server/ -o apidoc/",
    "start": "node ./dist/server.js",
    "cluster": "node ./dist/cluster.js",
    "hmr": "ng serve -o -hmr",
    "build": "ng build --prod && ngc && webpack --config webpack.server.config.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
