  "scripts": {
    "ng": "ng",
    "apidoc": "apidoc -i server/ -o apidoc/ && http-server apidoc/",
    "update-documentation": "apidoc -i server/ -o apidoc/",
    "start": "node ./dist/server.js",
    "cluster": "node ./dist/cluster.js",
    "hmr": "ng serve -o -hmr --proxy-config proxy.conf.json",
    "build": "ng build --prod && ng build -app 1 --prod --output-hashing=false && webpack --config webpack.server.config.js --progress --colors",
    "test": "snyk test && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "npm run build && npm start",
    "prod-cluster": "npm run build && npm run cluster",
    "all-tests": "ng test --watch=false && ng e2e",
    "continuous-integration": "npm run build",
    "heroku-postbuild": "npm run build",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "private": true,
